{
    "name": "r4d",
    "displayName": "R4d",
    "description": "Text oriented macro processor",
    "version": "0.1.0",
    "author": {
        "name": "simoncreek",
        "email": "simoncreek@tutanota.com"
    },
    "license": "MIT",
    "publisher": "simoncreek",
    "engines": {
        "vscode": "^1.60.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/simhyeon/r4d-vscode"
    },
    "keywords": ["cli","text processing", "macro"],
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "r4d",
            "aliases": ["r4d", "r4d"],
            "extensions": [".r4d"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "r4d",
            "scopeName": "source.r4d",
            "path": "./syntaxes/r4d.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "r4d",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}